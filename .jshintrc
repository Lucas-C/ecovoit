{
    // Inspired by airbnb/javascript
    "indent": 4,
    "strict": true,
    "newcap": true,
    "eqeqeq": true,
    "latedef": true,
    "undef": true,
    "unused": true,
    // Avoid "Bad line breaking before '+'"
    "laxbreak": true,
    // Code complexity control
    "maxparams": 4,
    "maxdepth": 3,
    //"maxcomplexity": 5,
    // Allow logging to the console & other globals from the libs used
    "browser": true,
    "globals": {
        "escape": false,
        "console": false,
        "_": false,
        "_paq": false,
        "google": false
    },
    "browserify": true,
    "jquery": true
}

